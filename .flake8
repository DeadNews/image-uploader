[flake8]
max-line-length = 88
max-complexity = 18
ignore =
    # Whitespace before ':'
    E203
    # Line too long
    E501
    # f-string without any placeholders
    F541
    # Line break occurred before a binary operator
    W503
per-file-ignores =
    # Imported but unused
    __init__.py: F401
